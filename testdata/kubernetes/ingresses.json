{
  "items": [
    {
      "metadata": {
        "name": "nginx-ingress",
        "namespace": "default"
      },
      "spec": {
        "rules": [
          {
            "host": "app.example.com",
            "http": {
              "paths": [
                {
                  "path": "/",
                  "pathType": "Prefix",
                  "backend": {
                    "service": {
                      "name": "nginx",
                      "port": {
                        "number": 80
                      }
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
